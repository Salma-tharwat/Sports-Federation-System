<Window x:Class="SFS.Edit_Player"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SFS"
        mc:Ignorable="d"
        Title="Edit_Player" Height="350" Width="500">
    <Grid Margin="4,0,2,3">
        <Grid.Background>
            <ImageBrush ImageSource="backgrd.png"/>
        </Grid.Background>
        <Button x:Name="back" BorderThickness="0" Content="" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Margin="-1,268,0,0" Height="48" Click="button4_Click">
            <Button.Background>
                <ImageBrush ImageSource="back.png"/>
            </Button.Background>
        </Button>
        <Button x:Name="button3" BorderThickness="0" Content="" HorizontalAlignment="Right" Width="30" Height="Auto" Margin="0,0,455.333,294.333" Click="button3_Click">
            <Button.Background>
                <ImageBrush ImageSource="wrongg.png"/>
            </Button.Background>
        </Button>
        <ComboBox x:Name="choose" Margin="98,92,177,199" Height="32" SelectionChanged="ComboBox_SelectionChanged" FontSize="16" FontFamily="Traditional Arabic" >>
            <ComboBoxItem>Mobile_Number</ComboBoxItem>
            <ComboBoxItem>Medical_Form</ComboBoxItem>
            <ComboBoxItem>Team_Name</ComboBoxItem>
            <ComboBoxItem>Results</ComboBoxItem>
        </ComboBox>
        <Button x:Name="next" Content="" HorizontalAlignment="Left" Width="71" Margin="415,268,0,2.333" Click="button1_Click" BorderThickness="0">
            <Button.Background>
                <ImageBrush ImageSource="next.png"/>
            </Button.Background>
        </Button>
        <Label x:Name="label2_Copy11" Content="Change:" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="2,84,0,0" FontFamily="Traditional Arabic" FontSize="22" FontWeight="Bold" Foreground="#FF34A5B0"/>

    </Grid>
</Window>
